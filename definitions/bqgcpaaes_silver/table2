-- models/tables/minha_tabela.sqlx

config {
  type: "table",
  schema: "meu_schema",
  tags: ["tabelas_base"],

  bigquery: {
    partitionBy: "DATE(data)",  -- particionamento por dia, baseado no campo data
    clusterBy: ["id"]           -- clustering pelo campo id
  }
}

select
  cast(null as int64)      as id,
  cast(null as string)     as nome,
  cast(null as timestamp)  as data